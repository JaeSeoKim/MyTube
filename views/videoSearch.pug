extends layouts/main
include mixin/videoBlock


block content
  .search__header
    h3 Searching By:  #{searchingBy}
  .search__video
    if videos.length === 0
      h5 h5 No Videos Found
    each item in videos
      +videoBlock({
        id:item.id,
        file:item.fileUrl,
        title:item.title,
        views:item.views
      })
